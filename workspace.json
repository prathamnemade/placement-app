{
  "version": 2,
  "cli": { "defaultCollection": "@nrwl/react" },
  "defaultProject": "placement-app",
  "generators": {
    "@nrwl/react": {
      "application": { "style": "scss", "linter": "eslint", "babel": true },
      "component": { "style": "scss" },
      "library": { "style": "scss", "linter": "eslint" }
    }
  },
  "projects": {
    "placement-app": {
      "root": "apps/placement-app",
      "sourceRoot": "apps/placement-app/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "stylePreprocessorOptions": {
            "includePaths": ["libs/shared/styles/src/lib"]
          },
          "extractCss": true,
          "options": {
            "outputPath": "dist/apps/placement-app",
            "index": "apps/placement-app/src/index.html",
            "main": "apps/placement-app/src/main.tsx",
            "polyfills": "apps/placement-app/src/polyfills.ts",
            "tsConfig": "apps/placement-app/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": ["libs/shared/styles/src"]
            },
            "sassOptions": { "includePaths": ["libs/shared/styles/src"] },
            "extractCss": true,
            "assets": [
              "apps/placement-app/src/favicon.ico",
              "apps/placement-app/src/assets",
              {
                "input": "libs/shared/assets/src/",
                "glob": "**/*",
                "output": "/assets/"
              }
            ],
            "styles": [
              "libs/shared/styles/src/index.scss",
              "apps/placement-app/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/placement-app/src/environments/environment.ts",
                  "with": "apps/placement-app/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": { "buildTarget": "placement-app:build" },
          "configurations": {
            "production": { "buildTarget": "placement-app:build:production" }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/placement-app/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/placement-app"],
          "options": {
            "jestConfig": "apps/placement-app/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": { "configFolder": "apps/placement-app/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/placement-app",
            "config": { "configFolder": "apps/placement-app/.storybook" }
          },
          "configurations": { "ci": { "quiet": true } }
        }
      }
    },
    "placement-app-e2e": {
      "root": "apps/placement-app-e2e",
      "sourceRoot": "apps/placement-app-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/placement-app-e2e/cypress.json",
            "tsConfig": "apps/placement-app-e2e/tsconfig.e2e.json",
            "devServerTarget": "placement-app:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "placement-app:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/placement-app-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "shared-storybook": {
      "root": "libs/shared/storybook",
      "sourceRoot": "libs/shared/storybook/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/storybook/**/*.{ts,tsx,js,jsx}"]
