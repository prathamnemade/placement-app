{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "defaultProject": "placement-tci",
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "scss",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "scss"
      },
      "library": {
        "style": "scss",
        "linter": "eslint"
      }
    }
  },
  "projects": {
    "placement-tci": {
      "root": "apps/placement-tci",
      "sourceRoot": "apps/placement-tci/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "stylePreprocessorOptions": {
            "includePaths": [
              "libs/shared/styles/src/lib"
            ]
          },
          "extractCss": true,
          "options": {
            "outputPath": "dist/apps/placement-tci",
            "index": "apps/placement-tci/src/index.html",
            "main": "apps/placement-tci/src/main.tsx",
            "polyfills": "apps/placement-tci/src/polyfills.ts",
            "tsConfig": "apps/placement-tci/tsconfig.app.json",
            "stylePreprocessorOptions": {
              "includePaths": [
                "libs/shared/styles/src"
              ]
            },
            "sassOptions": {
              "includePaths": [
                "libs/shared/styles/src"
              ]
            },
            "extractCss": true,
            "assets": [
              "apps/placement-tci/src/favicon.ico",
              "apps/placement-tci/src/assets",
              {
                "input": "libs/shared/assets/src/lib",
                "glob": "**/*",
                "output": "assets"
              }
            ],
            "styles": [
              "libs/shared/styles/src/index.scss",
              "apps/placement-tci/src/styles.scss"
            ],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/placement-tci/src/environments/environment.ts",
                  "with": "apps/placement-tci/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "placement-tci:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "placement-tci:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/placement-tci/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/placement-tci"
          ],
          "options": {
            "jestConfig": "apps/placement-tci/jest.config.js",
            "passWithNoTests": true
          }
        },
        "storybook": {
          "executor": "@nrwl/storybook:storybook",
          "options": {
            "uiFramework": "@storybook/react",
            "port": 4400,
            "config": {
              "configFolder": "apps/placement-tci/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        },
        "build-storybook": {
          "executor": "@nrwl/storybook:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/placement-tci",
            "config": {
              "configFolder": "apps/placement-tci/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "placement-tci-e2e": {
      "root": "apps/placement-tci-e2e",
      "sourceRoot": "apps/placement-tci-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/placement-tci-e2e/cypress.json",
            "tsConfig": "apps/placement-tci-e2e/tsconfig.e2e.json",
            "devServerTarget": "placement-tci:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "placement-tci:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/placement-tci-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    },
    "shared-storybook": {
      "root": "libs/shared/storybook",
      "sourceRoot": "libs/shared/storybook/src",
      "projectType": "library",
      "targets": {
