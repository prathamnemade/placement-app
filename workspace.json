          ],
          "options": {
            "uiFramework": "@storybook/react",
            "outputPath": "dist/storybook/shared-storybook",
            "config": {
              "configFolder": "libs/shared/storybook/.storybook"
            }
          },
          "configurations": {
            "ci": {
              "quiet": true
            }
          }
        }
      }
    },
    "shared-assets": {
      "root": "libs/shared/assets",
